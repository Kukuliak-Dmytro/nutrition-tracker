    "react": "19.1.0",
    "react-burger-menu": "^3.1.0",
    "react-day-picker": "^9.11.1",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.64.0",
    "recharts": "^3.3.0",
    "select": "^1.1.2",
    "shadcn": "^3.4.2",
    "sonner": "^2.0.7",
    "supabase": "^2.45.5",
    "tailwind-merge": "^3.3.1",
    "textarea": "^0.3.0",
    "use-debounce": "^10.0.6",
    "zod": "^4.1.12"
